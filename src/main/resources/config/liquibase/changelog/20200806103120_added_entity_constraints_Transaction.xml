<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Transaction.
    -->
    <changeSet id="20200806103120-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="paper_id"
                                 baseTableName="transaction"
                                 constraintName="fk_transaction_paper_id"
                                 referencedColumnNames="id"
                                 referencedTableName="paper"/>

        <addForeignKeyConstraint baseColumnNames="brokerage_note_id"
                                 baseTableName="transaction"
                                 constraintName="fk_transaction_brokerage_note_id"
                                 referencedColumnNames="id"
                                 referencedTableName="brokerage_note"/>

    </changeSet>
</databaseChangeLog>
