entity Paper {
	code String
    name String
    marketValue Long
    position Position
}

entity Position {
	paper Paper
}

enum TransactionType {
    BUY, SELL
}

entity Broker {
	name String
}
entity BrokerageNote {
	number String
    broker Broker
    emoluments Long
    liquidation Long
    otherTaxes Long
    value Long
}

entity Transaction {
	quantity String
    value Long
    date LocalDate
	paper Paper
    type TransactionType
    brokerageNote BrokerageNote
}

relationship OneToMany {
	Paper{position} to Position{paper}
}

relationship OneToMany {
	Paper to Transaction{paper}
}

relationship OneToMany {
	Broker to BrokerageNote{broker}
}

relationship OneToMany {
	BrokerageNote to Transaction{brokerageNote}
}
