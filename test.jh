entity Paper {
	  code String
    name String
    marketValue Long
}

entity Position

enum TransactionType {
    BUY, SELL
}

entity Broker {
	name String
}

entity BrokerageNote {
	number String
    emoluments Long
    liquidation Long
    otherTaxes Long
    value Long
}

entity Transaction {
	quantity String
    value Long
    date LocalDate
	  paper Paper
    type TransactionType
}

relationship OneToMany {
	Paper to Position{paper}
}

relationship OneToMany {
	Paper to Transaction{paper}
}

relationship OneToMany {
	Broker to BrokerageNote{broker}
}

relationship OneToMany {
	BrokerageNote to Transaction{brokerageNote}
}

relationship ManyToOne {
  BrokerageNote{broker} to Broker
}